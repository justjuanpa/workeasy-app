{
  "expo": {
    "name": "workeasy-app",
    "slug": "workeasy-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/WorkEasyIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/WorkEasyIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.workeasy.App",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/0487dd34-82a2-45a0-b98b-dcf04c85537f"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0487dd34-82a2-45a0-b98b-dcf04c85537f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
